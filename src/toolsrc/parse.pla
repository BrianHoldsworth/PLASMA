    if opsp < 0; exit_err(ERR_INVAL|ERR_CODE|ERR_FRAME); fin
    return opstack[opsp]
end
def tos_op
    return opsp < 0 ?? 0 :: opstack[opsp-1]
end
def tos_op_prec(tos)
