    lparms     = 0
    rparms     = 0
    lambda_set = lambda_cnt
    setptr     = tknptr
    memset(@setseq, 0, 16)
    repeat
        setseq[lparms], drop = parse_value(NULL, LVALUE)
        if not setseq[lparms]; break; fin
        lparms++
